
@{
    ViewData["Title"] = "Index";
}
    
    <a asp-controller="Theme" asp-action="createTheme">Create</a>

    <table class="table">
        <tr><th>Name</th><th></th></tr>
        @foreach (var theme in Model)
        {
            <tr>
                <td>@theme.themeName</td>
                <td> <a class="btn btn-sm btn-primary" asp-action="deleteTheme" asp-route-userid="@theme.themeId">Delete</a></td>
            </tr>
        }
    </table>
